    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-twitter": "^2.0.8",
    "gatsby-remark-images-contentful": "^2.0.5",
    "gatsby-remark-prismjs": "^3.2.2",
    "gatsby-source-contentful": "^2.0.26",
    "gatsby-source-filesystem": "^2.0.18",
    "gatsby-transformer-remark": "^2.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "prismjs": "^1.15.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-pose": "^4.0.6",
    "styled-components": "^4.1.3"
  },
  "keywords": [
    "rw"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier -l {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "yarn run lint:css && yarn run lint:js && yarn run check-pretty",
    "clean": "rm -rf public .cache",
    "share": "gatsby develop --host $(ifconfig | awk '/inet 192\\.168\\.[0-9]+\\.[0-9]+/{print $2}') --port 8000"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-config-gatsby-standard": "^2.1.1",
    "prettier": "^1.16.1",
    "stylelint": "9.10.1",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2"
  }
